333                     f << setprecision(2) << v[x][y][z].r << {\cf22 " "} << setprecision(2) << v[x][y][z].g << {\cf22 " "} << setprecision(2) << v[x][y][z].b << {\cf22 " "} << setprecision(2) << v[x][y][z].a << {\cf22 "\\n"};\par
334                     {\cf20 // FACE 3}\par
335                     f << 4 << {\cf22 " "} << index + 0 << {\cf22 " "} << index + 4 << {\cf22 " "} << index + 7 << {\cf22 " "} << index + 3 << {\cf22 " "};\par
336                     f << setprecision(2) << v[x][y][z].r << {\cf22 " "} << setprecision(2) << v[x][y][z].g << {\cf22 " "} << setprecision(2) << v[x][y][z].b << {\cf22 " "} << setprecision(2) << v[x][y][z].a << {\cf22 "\\n"};\par
337                     {\cf20 // FACE 4}\par
